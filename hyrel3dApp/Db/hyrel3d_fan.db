# Fan control for T11=MK1 head - motor speed in steps, FanOn=30, FanOff=0
record(longout, "$(Sys)$(Dev)T$(Yoke)$(Head):Cmd-Fan")
{
	field(DESC, "Set Fan Speed")
	field(EGU,  "Hz")
    field(DTYP, "stream")
	field(DRVH, 35)
    field(OUT,  "@hyrel.proto setFan($(Sys)$(Dev),T$(Yoke)$(Head)) $(PORT) $(A)")
}