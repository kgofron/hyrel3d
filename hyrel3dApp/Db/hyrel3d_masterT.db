# Head = T12, where 1 is yoke, and 2 is head in that yoke
# Unsolicited temperature (one-head) ">RT :T12 25 0 0 0"
record (ai, "$(Sys)$(Dev)T$(Yoke)$(Head):T-RB") {
  field (DTYP, "stream")
  field(EGU,  "C")
  field(PREC, "0")    
  field (INP, "@hyrel.proto readTemp$(N)($(Sys)$(Dev),$(Heads)) $(PORT) $(A)")
  field (SCAN, "I/O Intr")
}
