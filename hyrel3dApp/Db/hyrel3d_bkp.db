# Set module
record(ao, "$(Sys)$(Dev)M-SP")
{
	field(DESC, "Set M104 module")
	field(EGU,  "Mcode")
	field(PREC, "0")
	field(DRVL, 1)
	field(DRVH, 200)
	field(VAL,  104)
}

# Set head
record(ao, "$(Sys)$(Dev)H-SP")
{
	field(DESC, "Set T12 head")
	field(EGU,  "Tcode")
	field(PREC, "0")
	field(DRVL, 1)
#	field(DRVH, 20)
	field(VAL,  12)
}

# PV records related to extrusion rate

record(longout, "$(Sys)$(Dev)SetRate")
{
    field(DESC, "SCPI enable event status")
    field(DTYP, "stream")
    field(OUT,  "@hyrel.proto setRate($(Sys)$(Dev)) $(PORT) $(A)")
}

# Set M723-code for extrusion
record(ao, "$(Sys)$(Dev)M7-SP")
{
	field(DESC, "Set M723 module")
	field(EGU,  "Mcode")
	field(PREC, "0")
	field(DRVL, 1)
	field(DRVH, 800)
	field(VAL,  723)
}

# Set extrusion quantity in motor steps, 200 x 32=6400 for full rotation
record(ao, "$(Sys)$(Dev)Qty-SP")
{
	field(DESC, "Set amount")
	field(EGU,  "uL")
	field(PREC, "0")
	field(DRVL, 1)
#	field(DRVH, 6400)
	field(VAL,  6400)
}

